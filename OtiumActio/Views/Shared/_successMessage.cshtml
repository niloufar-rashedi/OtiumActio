
<div>
    @if (ViewData["Error"] != null)
    {
        <div class="alert alert-danger" role="alert">
            <h4 class="alert-heading"> @ViewData["Error"].ToString()</h4>
            <a href="@Url.Action("Index","Activity")">Gå till aktivitetslista</a>
        </div>
    }
    @if (TempData["Success"] != null)
    {
        <div class="alert alert-success" role="alert">
            <h4 class="alert-heading"> @TempData["Success"].ToString()</h4>
            <a href="@Url.Action("Index","Activity")">Gå till aktivitetslista</a>

        </div>
    }
</div>